{
  "name": "@alexandria/relay_selector",
  "version": "0.1.0",
  "description": "A Rust WASM library for algorithmically choosing Nostr relays",
  "exports": {
    ".": {
      "types": "./lib/relay_selector.d.ts",
      "default": "./lib/relay_selector.js"
    }
  },
  "tasks": {
    "build": "wasm-pack build --target bundler --out-dir pkg/bundler",
    "wasmbuild": "deno run -A jsr:@deno/wasmbuild@0.19.3 --inline --project relay_selector",
    "check": "deno task wasmbuild --check"
  }
}
